ics, \"CHECKED\");\r\n        //checkedTopics 选中的话题\r\n\r\n        //generate filter list\r\n        let tmpArr = [];\r\n        for(let index = 0; index < this.listDemo.length; index++) {\r\n          let topics = this.listDemo[index].topics;\r\n          console.log(topics, '1.TOPICS CHANGED');\r\n          for (let j = 0; j < this.checkedTopics.length; j++) {\r\n            console.log(topics, 'TOPICS');\r\n            if (topics.includes(this.checkedTopics[j]) == true){\r\n              tmpArr.push(this.listDemo[index])\r\n            }\r\n          }\r\n\r\n        // this.dataListDisplayed\r\n        }\r\n\r\n        function unique (arr) {\r\n          return Array.from(new Set(arr))\r\n        }\r\n        console.log('去重后', unique(tmpArr));\r\n\r\n        // function unique (arr) {\r\n        //   return Array.from(new Set(arr))\r\n        // }\r\n        // var arr = [1,1,'true','true',true,true,15,15,false,false, undefined,undefined, null,null, NaN, NaN,'NaN', 0, 0, 'a', 'a',{},{}];\r\n        // console.log(unique(arr))\r\n\r\n\r\n        // console.log('FILTERED', this.dataListDisplayed)\r\n      }      \r\n    },\r\n    \r\n    setup() {\r\n      let listDemo = [\r\n            {\"agt\": \"agt1\", \"date\": \"2000-01-01\", \"topics\":[\"human rights\"]},\r\n            {\"agt\": \"agt2\", \"date\": \"2001-01-01\", \"topics\":[\"human rights\", \"ceasefires\"]},\r\n            {\"agt\": \"agt3\", \"date\": \"2002-01-01\", \"topics\":[\"ceasefires\"]},\r\n            {\"agt\": \"agt4\", \"date\": \"2012-01-01\", \"topics\":[\"human rights\", \"ceasefires\"]}\r\n        ];\r\n\r\n      \r\n       let dateOptions = [];\r\n       let topicOptions = [];\r\n       let agtList = [];\r\n\r\n        for (let i = 0; i < listDemo.length; i++) {\r\n            \r\n            dateOptions.push(listDemo[i].date);\r\n            agtList.push(listDemo[i].agt);\r\n\r\n            let topics = listDemo[i].topics;\r\n            for (let j = 0; j < topics.length; j++){\r\n                // console.log(topics[j]);\r\n                if (topicOptions.includes(topics[j]) == false) {\r\n                    topicOptions.push(topics[j])\r\n                }\r\n            }\r\n            // console.log(topicOptions);\r\n        }\r\n        console.log(agtList, 'AGREMENT');\r\n        console.log(dateOptions, 'DATES')\r\n\r\n        return {\r\n        listDemo, dateOptions, topicOptions, agtList\r\n      }\r\n\r\n    }\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\nh3 {\r\n    font-size: 16px;\r\n    text-align: left;\r\n    padding-top: 20px;\r\n    margin-left: 60px;\r\n    margin-bottom: 0px;\r\n}\r\n\r\nh4 {\r\n    text-align: left;\r\n    font-size:18px;\r\n    font-weight: 500;\r\n    padding: 0px;\r\n    margin: 0px;\r\n}\r\n\r\np {\r\n    text-align: left;\r\n}\r\n\r\n.el-checkbox-group {\r\n    text-align: left;\r\n}\r\n\r\n.el-checkbox {\r\n    display: block; \r\n    padding-top: 20px;\r\n    margin-left: 60px;\r\n    margin-right: 25px;\r\n    width: 200px;\r\n    padding-left: 0px;\r\n    text-align: left;\r\n}\r\n\r\n.el-divider--vertical{\r\n  display:inline-block;\r\n  width:2px;\r\n  height:30em;\r\n  margin:0 8px;\r\n  vertical-align:middle;\r\n  position:relative;\r\n}\r\n\r\n.el-main {\r\n  padding-top: 40px;\r\n  padding-left: 50px;\r\n}\r\n\r\n.doc-list {\r\n  margin-bottom: 40px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}