 = country;\n        // console.log(data, \"本国的所有信息\")\n      }\n    }\n\n    //得到所有agreement\n    let topic = '';\n    let topicOptions = [];\n    let agtName = '';\n    let agtTime = '';\n    // let agtTimeExact = ''\n\n    for (let [index, item] of data.topics.entries()) {\n      // 得到所有topic\n      topic = item.text;\n      topicOptions.push(topic);\n      // topicID = item.id\n\n      console.log('查找', agreementList, index);\n      for (let agt of item.agreements) {\n        if (agtName != agt.name) {\n          //得到agreement名称\n          agtName = agt.name;\n          console.log(agtName);\n\n          //得到时间\n          let tmp = agt.date.toString();\n          let YEAR = tmp.slice(0, 4);\n          let MONTH = tmp.slice(4, 6);\n          let DAY = tmp.slice(6, 7);\n\n          // console.log(YEAR, MONTH, DAY, '时间转换')\n          agtTime = YEAR + '-' + MONTH + '-' + DAY;\n          // agtTimeExact = new Date (agtTime)\n          // console.log(agtTimeExact, 'TIME')\n\n          agreementList.push({\n            \"agt\": agtName,\n            \"date\": agtTime,\n            \"topics\": topicOptions\n          });\n        }\n      }\n\n      // console.log(agreementList, '得到的列表')\n    }\n\n    return {\n      countryName,\n      data,\n      agreementList,\n      topicOptions\n    };\n  }\n};","map":{"version":3,"mappings":";AAqEA;AACA,SAASA,QAAO,QAAS,YAAW;;AAEpC;AACA,OAAOC,aAAY,MAAO,gCAA+B;AACzD,OAAOC,aAAY,MAAO,gCAA+B;AACzD;AACA;;AAEA;AACA,OAAOC,SAAQ,MAAO,uBAAsB;AAE5C,eAAgB;EACZC,UAAU,EAAE;IACRH,aAAa;IAAEC;EACnB,CAAC;EAEDG,IAAI,GAAG;IACH,OAAM;MACNC,KAAK,EAAE,IAAI,CAACC,WAAW;MAEvBC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE,IAAI,CAACC,YAAY;MAChCC,MAAM,EAAE,IAAI,CAACD,YAAY;MACzBE,eAAe,EAAE,IAAI;MAErB;MACAC,iBAAiB,EAAE,IAAI,CAACC;IACxB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,oBAAoB,CAACC,GAAG,EAAE;MAC1B,IAAI,CAACR,aAAY,GAAIQ,GAAE,GAAI,IAAI,CAACP,YAAW,GAAI,EAAE;MACjD,IAAI,CAACE,eAAc,GAAI,KAAK;IAC9B,CAAC;IACDM,yBAAyB,CAACC,KAAK,EAAE;MAC/B,IAAIC,YAAW,GAAID,KAAK,CAACE,MAAM;MAC/B,IAAI,CAA