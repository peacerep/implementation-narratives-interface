{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRoute } from 'vue-router';\n\n// 导入组件\nimport trackerHeader from '@/components/trackerHeader.vue';\nimport trackerFooter from '@/components/trackerFooter.vue';\nimport asideFilter from '@/components/asideFilter.vue';\nimport countryHeader from '@/components/countryHeader.vue';\n// import docList from '@/components/docList.vue'\n\n// 导入数据\nimport countries from '@/data/countries.json';\nexport default {\n  components: {\n    trackerHeader,\n    trackerFooter,\n    asideFilter\n  },\n  data() {\n    return {\n      title: this.countryName,\n      dataListDisplayed: this.agreementList,\n      //父组件传data给filter\n      allAgt: this.agreementList,\n      allOptions: this.topicOptions\n    };\n  },\n  methods: {\n    changeDisplayList(newList) {\n      this.dataListDisplayed = newList;\n    }\n  },\n  setup() {\n    //pass in country title through route\n    const $route = useRoute();\n    let countryName = $route.query.title;\n    let data = {};\n    let agreementList = [];\n\n    //初始化本页需要的数据\n    for (let country of countries.countries) {\n      console.log(country.name, \"log country\");\n      if (country.name == countryName) {\n        data = country;\n      }\n    }\n\n    //得到所有agreement\n    let topic = '';\n    let topicOptions = [];\n    let agtName = '';\n    // let agtTimeExact = ''\n\n    let agtList = [];\n    for (let [j, item] of data.topics.entries()) {\n      topic = item.text;\n      topicOptions.push(topic);\n      console.log('话题', j, topic);\n      for (let [i, agt] of item.agreements.entries()) {\n        agtName = agt.name;\n        let topicTagList = [];\n        let agtTime = '';\n        console.log(i);\n\n        //得到文件名后，判断agtList里是否已经有这个文件\n        if (agtList.includes(agtName) == false) {\n          agtList.push(agtName);\n\n          //对这个文件添加话题表\n          topicTagList.push(topic);\n\n          //得到时间\n          let tmp = agt.date.toString();\n          let YEAR = tmp.slice(0, 4);\n          let MONTH = tmp.slice(4, 6);\n          let DAY = tmp.slice(6, 8);\n          agtTime = YEAR + '-' + MONTH + '-' + DAY;\n          // agtTimeExact = new Date (agtTime)\n\n          agreementList.push({\n            'agt': agtName,\n            'date': agtTime,\n            \"topics\": topicTagList\n          });\n        }\n      }\n      // console.log('列表', agreementList)\n    }\n\n    return {\n      countryName,\n      data,\n      agreementList,\n      topicOptions\n    };\n  }\n};","map":{"version":3,"mappings":";AAkDA,SAASA,QAAO,QAAS,YAAW;;AAEpC;AACA,OAAOC,aAAY,MAAO,gCAA+B;AACzD,OAAOC,aAAY,MAAO,gCAA+B;AACzD,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,aAAY,MAAO,gCAA+B;AACzD;;AAEA;AACA,OAAOC,SAAQ,MAAO,uBAAsB;AAE5C,eAAgB;EACZC,UAAU,EAAE;IACRL,aAAa;IAAEC,aAAa;IAAEC;EAClC,CAAC;EAEDI,IAAI,GAAG;IACH,OAAM;MACFC,KAAK,EAAE,IAAI,CAACC,WAAW;MACvBC,iBAAiB,EAAE,IAAI,CAACC,aAAa;MAErC;MACAC,MAAM,EAAE,IAAI,CAACD,aAAa;MAC1BE,UAAU,EAAE,IAAI,CAACC;IACrB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,iBAAiB,CAACC,OAAO,EAAE;MACvB,IAAI,CAACP,iBAAgB,GAAIO,OAAM;IACnC;EACJ,CAAC;EAEDC,KAAK,GAAG;IACJ;IACA,MAAMC,MAAK,GAAInB,QAAQ,EAAC;IACxB,IAAIS,WAAU,GAAIU,MAAM,CAACC,KAAK,CAACZ,KAAI;IACnC,IAAID,IAAG,GAAI,CAAC;IACZ,IAAII,aAAY,GAAI,EAAC;;IAErB;IACA,KAAK,IAAIU,OAAM,IAAKhB,SAAS,CAACA,SAAS,EAAE;MACrCiB,OAAO,CAACC,GAAG,CAACF,OAAO,CAACG,IAAI,EAAE,aAAa;MACvC,IAAIH,OAAO,CAACG,IAAG,IAAKf,WAAW,EAAE;QAC7BF,IAAG,GAAIc,OAAM;MACjB;IACJ;;IAEA;IACA,IAAII,KAAI,GAAI,EAAC;IACb,IAAIX,YAAW,GAAI,EAAC;IACpB,IAAIY,OAAM,GAAI,EAAC;IACf;;IAEA,IAAIC,OAAM,GAAI,EAAC;IACf,KAAK,IAAI,CAACC,CAAC,EAAEC,IAAI,KAAKtB,IAAI,CAACuB,MAAM,CAACC,OAAO,EAAE,EAAE;MACrCN,KAAI,GAAII,IAAI,CAACG,IAAG;MAChBlB,YAAY,CAACmB,IAAI,CAACR,KAAK;MACvBH,OAAO,CAACC,GAAG,CAAC,IAAI,EAAEK,CAAC,EAAEH,KAAK;MAE1B,KAAK,IAAI,CAACS,CAAC,EAAEC,GAAG,KAAKN,IAAI,CAACO,UAAU,CAACL,OAAO,EAAE,EAAC;QAC3CL,OAAM,GAAIS,GAAG,CAACX,IAAG;QACjB,IAAIa,YAAW,GAAI,EAAC;QACpB,IAAIC,OAAM,GAAI,EAAC;QACfhB,OAAO,CAACC,GAAG,CAACW,CAAC;;QAEb;QACA,IAAIP,OAAO,CAACY,QAAQ,CAACb,OAAO,